package game.interaction;

public class Goblin {
	

	private int hp;
	private int mp;
	private int atk;
	private int def;
	
	
	public Goblin() {
		this.hp = 10;
		this.atk = 5;
		this.def = 1;
		
	}


	public int getHp() {
		return hp;
	}


	public void setHp(int hp) {
		this.hp = hp;
	}


	public int getMp() {
		return mp;
	}


	public void setMp(int mp) {
		this.mp = mp;
	}


	public int getAtk() {
		return atk;
	}


	public void setAtk(int atk) {
		this.atk = atk;
	}


	public int getDef() {
		return def;
	}


	public void setDef(int def) {
		this.def = def;
	}

}
