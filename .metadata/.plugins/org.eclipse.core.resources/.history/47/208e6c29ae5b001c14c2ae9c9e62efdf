package garbagecollection;

public class B_Main_201p {

	public static void main(String[] args) {
		// 6개 객체생성
		A_item i1 = new Aitem(1);
		A_item i2 = new Aitem(2);
		i2 = null;
		A_item i3 = new Aitem(3);
		// 메모리 중간에 회수하도록 강제명령
		System.gc());
		A_item i4 = new Aitem(4);
		i1 = null;
		A_item i5 = new Aitem(5);
		A_item i6 = new Aitem(6);
		
		

	}

}
