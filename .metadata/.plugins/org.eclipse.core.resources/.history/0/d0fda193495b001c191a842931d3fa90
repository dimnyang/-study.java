package structure;

public class E_Structure03_190p {
	
	public static void cat(); {
		System.out.println("이름 : " + name + "이고, 색상은 " + color + "입니다.");
	}

	public static void main(String[] args) {
		
		
		cat a = new cat();
		
		a.name = "만두";
		a.color= "노란색";
		
		cat b = new cat();
		
		b.name = "두부";
		b.color = "하얀색";
		
		
		getInfo(a);
		getInfo(b);

	}

}
