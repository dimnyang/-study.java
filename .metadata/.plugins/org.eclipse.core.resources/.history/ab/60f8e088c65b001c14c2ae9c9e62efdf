package accessmodifier;

public class Warrior {

	// hp, mp, lv, atk, def, exp, name;
	public String name;
	public int hp;
	public int mp;
	public int lv;
	public int atk;
	public int def;
	public int exp;
	
	생성자 규칙 : 클래스명과 동일한 이름의 메서드를 만들되, 리턴자료형이 없음
	
	public Warrior (String n) {
		name = n;
		hp = 20;
		mp = 10;
		lv = 1;
		atk = 3;
		def = 0;
		exp = 0;
		
		System.out.println("전사 생성완료");
		System.out.println("아이디 : " + name + "레벨 : " + hp);
		
		
	}
	

}
