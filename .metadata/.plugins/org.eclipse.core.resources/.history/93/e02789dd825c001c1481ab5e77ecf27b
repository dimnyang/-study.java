package overriding;

public class Main01_265p {

	public static void main(String[] args) {
		
		Dog d1 = new Dog();
		d1.howl();
		d1.name = "멍멍이";
		d1.age = 5;
		d1.food ="개사료";
		
		
		Cat c1 = new Cat();
		c1.name = "만두";
		c1.age = 7;
		c1.food ="고냥이 사료";
		c1.weight = 10;
		c1.howl();
		
		
		d1.getInfo();
		c1.getInfo();

	}

}
