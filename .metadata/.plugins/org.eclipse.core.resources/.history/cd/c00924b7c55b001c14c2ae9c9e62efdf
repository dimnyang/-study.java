package accessmodifier;

public class Warrior {

	// hp, mp, lv, atk, def, exp, name;
	public String name;
	public int hp;
	public int mp;
	public int lv;
	public int atk;
	public int def;
	public int exp;
	
	public Warrior () {
		name = "n";
		hp = 20;
		mp = 10;
		lv = 1;
		atk = 3;
		def = 0;
		exp = 0;
	}
	

}
