import java.util.Scanner;

public class z {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		System.out.println("2~9단 사이의 숫자를 입력해주세요.");
		int dan = scan.nextInt();
		if ((1 < dan) && (dan < 10)); {
			for(int i = 1; i < 10; i++) {
				System.out.println(dan + "*" + i + " =" + dan*i);
			}
		}
	
	}}