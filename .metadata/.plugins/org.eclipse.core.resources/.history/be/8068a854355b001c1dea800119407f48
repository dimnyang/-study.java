
public class zz {
 public static void plus2(int c, int num) {
	 System.out.println( c + num);
 }
	public static void main(String[] args) {

		//int c, int num을 받아서
		// system.out.println( c + num);만 실행하고 종료되는
	    // plus2 메서드를 정의해주신다음
		// main내부에서 c에는 10, num에는 20을 전달해 실행하도록
		// 코드를 작성해보세요.,
		
		plus2(10,20);

	}

}
