package zzzz;

public class Main {

	public static void main(String[] args) {
		Dog d1 = new Dog();
		d1.name = "멍멍이";
		d1.age = 1;
		d1.color = "노란색";
		d1.breed = "리트리버";
		d1.howl();
		
		d1.getInfo();
		
		Cat c1 = new Cat ();
		c1.name = "야옹이";
		c1.age = 5;
		c1.color = "치즈색";
		c1.food = "캣닢";
		c1.howl();
		
		c1.getInfo();

	}

}
